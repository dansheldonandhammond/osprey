                    <script>
                      function initCompleteSystemSlider() {
                        const slider = document.querySelector('.complete-system-slider');
                        if (!slider) return;

                        const track = slider.querySelector('.slider-track');
                        const items = slider.querySelectorAll('.slider-item');
                        const prevBtn = slider.querySelector('.slider-prev');
                        const nextBtn = slider.querySelector('.slider-next');
                        const dotsContainer = slider.querySelector('.slider-dots'); // optional

                        if (!track || items.length === 0) return;

                        let currentIndex = 0;
                        let visibleItems = 1;
                        let maxIndex = 0;
                        let step = 0; // px to move per item

                        function calcStep() {
                          const first = items[0];
                          if (!first) return 0;
                          // Force a reflow to ensure latest styles are applied
                          void first.offsetHeight;
                          const rect = first.getBoundingClientRect();
                          const cs = getComputedStyle(first);
                          const marginLeft = parseFloat(cs.marginLeft) || 0;
                          const marginRight = parseFloat(cs.marginRight) || 0;
                          const margin = marginLeft + marginRight;
                          const width = rect.width + margin;
                          // Return 0 if width is invalid (element not rendered yet)
                          return width > 0 ? width : 0;
                        }

                        function recalc() {
                          const viewport = slider.querySelector('.slider-container') || slider;
                          const viewportWidth = viewport.clientWidth || slider.clientWidth;
                          
                          if (viewportWidth <= 0 || items.length === 0) {
                            visibleItems = items.length;
                            maxIndex = 0;
                            return;
                          }
                          
                          // Responsive breakpoints: determine how many items to show
                          if (window.innerWidth < 1024) {
                            visibleItems = 2; // Mobile and Tablet: 2 items
                          } else {
                            visibleItems = 3; // Desktop: 3 items
                          }
                          
                          // Cap at actual number of items if we have fewer
                          visibleItems = Math.min(visibleItems, items.length);
                          
                          // Get margin from first item (or use 0 if not available)
                          const firstItem = items[0];
                          let itemMargin = 0;
                          if (firstItem) {
                            const cs = getComputedStyle(firstItem);
                            const marginLeft = parseFloat(cs.marginLeft) || 0;
                            const marginRight = parseFloat(cs.marginRight) || 0;
                            itemMargin = marginLeft + marginRight;
                          }
                          
                          // Calculate the exact width each item should be to fit visibleItems perfectly
                          // Account for margins between items (visibleItems - 1 gaps)
                          const totalMarginSpace = itemMargin * (visibleItems - 1);
                          const availableWidth = viewportWidth - totalMarginSpace;
                          const itemWidth = Math.floor(availableWidth / visibleItems);
                          
                          // Apply the calculated width to all items to ensure they fit exactly
                          items.forEach(item => {
                            item.style.width = itemWidth + 'px';
                            item.style.flexShrink = '0';
                            item.style.flexGrow = '0';
                          });
                          
                          // Recalculate step with new item widths
                          step = calcStep();
                          
                          maxIndex = Math.max(0, items.length - visibleItems);
                          // keep currentIndex in bounds after recalculation
                          currentIndex = Math.max(0, Math.min(currentIndex, maxIndex));
                        }

                        function updateDots() {
                          if (!dotsContainer) return;
                          const totalDots = Math.ceil(items.length / visibleItems);
                          dotsContainer.innerHTML = '';
                          for (let i = 0; i < totalDots; i++) {
                            const dot = document.createElement('button');
                            dot.className =
                              't-w-2 t-h-2 t-rounded-full t-border-0 t-cursor-pointer t-transition-colors t-duration-200 t-bg-gray-300';
                            dot.setAttribute('aria-label', `Go to slide ${i + 1}`);
                            if (i === Math.floor(currentIndex / visibleItems))
                              dot.classList.replace('t-bg-gray-300', 't-bg-black');
                            dot.addEventListener('click', () => {
                              goToSlide(i * visibleItems);
                            });
                            dotsContainer.appendChild(dot);
                          }
                        }

                        function updateButtons() {
                          // Always recalc first to ensure we have fresh values
                          recalc();
                          
                          // If step is still 0, items aren't rendered - hide arrows
                          if (step <= 0) {
                            if (prevBtn) {
                              prevBtn.style.display = 'none';
                              prevBtn.disabled = true;
                            }
                            if (nextBtn) {
                              nextBtn.style.display = 'none';
                              nextBtn.disabled = true;
                            }
                            if (dotsContainer) dotsContainer.style.display = 'none';
                            return;
                          }
                          
                          const atStart = currentIndex === 0;
                          const atEnd = currentIndex >= maxIndex;
                          
                          // Use actual DOM measurements as the primary check - most reliable
                          const viewport = slider.querySelector('.slider-container') || slider;
                          
                          // Calculate total width of all items
                          let totalItemsWidth = 0;
                          items.forEach(item => {
                            const rect = item.getBoundingClientRect();
                            const cs = getComputedStyle(item);
                            const margin = (parseFloat(cs.marginLeft) || 0) + (parseFloat(cs.marginRight) || 0);
                            totalItemsWidth += rect.width + margin;
                          });
                          
                          const viewportWidth = viewport.clientWidth || slider.clientWidth;
                          
                          // Check if total items width is wider than viewport (with tolerance)
                          const hasOverflow = totalItemsWidth > viewportWidth + 10; // 10px tolerance for margins/rounding
                          
                          // Also verify with count check as backup
                          const hasOverflowByCount = items.length > visibleItems;
                          
                          // Show arrows only if BOTH checks agree (DOM measurement is primary)
                          const shouldShowArrows = hasOverflow && hasOverflowByCount;
                          
                          if (prevBtn) {
                            if (shouldShowArrows) {
                              prevBtn.style.setProperty('display', 'flex', 'important');
                              prevBtn.style.setProperty('visibility', 'visible', 'important');
                              prevBtn.style.setProperty('opacity', atStart ? '0.5' : '1', 'important');
                              prevBtn.disabled = atStart;
                            } else {
                              prevBtn.style.setProperty('display', 'none', 'important');
                              prevBtn.style.setProperty('visibility', 'hidden', 'important');
                              prevBtn.style.setProperty('opacity', '0', 'important');
                              prevBtn.disabled = true;
                            }
                          }
                          if (nextBtn) {
                            if (shouldShowArrows) {
                              nextBtn.style.setProperty('display', 'flex', 'important');
                              nextBtn.style.setProperty('visibility', 'visible', 'important');
                              nextBtn.style.setProperty('opacity', atEnd ? '0.5' : '1', 'important');
                              nextBtn.disabled = atEnd;
                            } else {
                              nextBtn.style.setProperty('display', 'none', 'important');
                              nextBtn.style.setProperty('visibility', 'hidden', 'important');
                              nextBtn.style.setProperty('opacity', '0', 'important');
                              nextBtn.disabled = true;
                            }
                          }
                          if (dotsContainer) {
                            dotsContainer.style.display = shouldShowArrows ? '' : 'none';
                          }
                        }

                        function updateSlider() {
                          recalc(); // Ensure calculations are up to date before updating
                          if (step > 0) {
                            track.style.transform = `translateX(${-currentIndex * step}px)`;
                          }
                          updateDots();
                          updateButtons(); // Call updateButtons after recalc to ensure fresh values
                        }

                        function goToSlide(index) {
                          recalc(); // ensure visibleItems/maxIndex are fresh
                          currentIndex = Math.max(0, Math.min(index, maxIndex));
                          updateSlider();
                        }

                        function nextSlide() {
                          goToSlide(currentIndex + visibleItems);
                        } // page by view
                        function prevSlide() {
                          goToSlide(currentIndex - visibleItems);
                        }

                        nextBtn && nextBtn.addEventListener('click', nextSlide);
                        prevBtn && prevBtn.addEventListener('click', prevSlide);

                        // Touch/swipe
                        let startX = 0,
                          currentX = 0,
                          dragging = false;
                        track.addEventListener('touchstart', (e) => {
                          startX = e.touches[0].clientX;
                          dragging = true;
                        });
                        track.addEventListener('touchmove', (e) => {
                          if (dragging) currentX = e.touches[0].clientX;
                        });
                        track.addEventListener('touchend', () => {
                          if (!dragging) return;
                          dragging = false;
                          const diffX = startX - currentX;
                          if (Math.abs(diffX) > 50) diffX > 0 ? nextSlide() : prevSlide();
                        });

                        // Keyboard
                        slider.addEventListener('keydown', (e) => {
                          if (e.key === 'ArrowLeft') prevSlide();
                          if (e.key === 'ArrowRight') nextSlide();
                        });

                        // Resize handler with debouncing and proper recalculation
                        let resizeTimeout;
                        let lastWidth = window.innerWidth;
                        const handleResize = () => {
                          clearTimeout(resizeTimeout);
                          resizeTimeout = setTimeout(() => {
                            const currentWidth = window.innerWidth;
                            // Always recalculate on resize to catch responsive class changes
                            lastWidth = currentWidth;
                            // Force recalculation of step (items may have resized due to responsive classes)
                            step = 0; // Reset step to force recalculation
                            recalc();
                            updateSlider();
                            // Double-check after a short delay to ensure layout is stable
                            setTimeout(() => {
                              recalc();
                              updateButtons();
                            }, 50);
                          }, 150); // Debounce resize events
                        };
                        window.addEventListener('resize', handleResize);
                        
                        // Also listen for orientation changes on mobile
                        window.addEventListener('orientationchange', () => {
                          setTimeout(() => {
                            step = 0; // Reset step
                            recalc();
                            updateSlider();
                            setTimeout(() => {
                              recalc();
                              updateButtons();
                            }, 100);
                          }, 100);
                        });

                        // Init - use multiple frames to ensure DOM is fully ready and styles are applied
                        const initSlider = () => {
                          recalc();
                          updateSlider();
                          // Wait for next frame to ensure responsive classes have been applied
                          requestAnimationFrame(() => {
                            recalc();
                            updateButtons();
                            // One more check after a short delay to catch any late layout changes
                            setTimeout(() => {
                              recalc();
                              updateButtons();
                            }, 200);
                          });
                        };
                        
                        // Wait for DOM to be ready
                        if (document.readyState === 'loading') {
                          document.addEventListener('DOMContentLoaded', () => {
                            requestAnimationFrame(initSlider);
                          });
                        } else {
                          requestAnimationFrame(initSlider);
                        }
                      }
                      document.readyState === 'loading'
                        ? document.addEventListener('DOMContentLoaded', initCompleteSystemSlider)
                        : initCompleteSystemSlider();
                    </script>

                    <script>
                      // Add to cart functionality for complete system slider
                      document.addEventListener('DOMContentLoaded', function() {
                        const addToCartButtons = document.querySelectorAll('.complete-system-add-to-cart');
                        
                        addToCartButtons.forEach(button => {
                          // Prevent touch events from triggering slider navigation
                          button.addEventListener('touchstart', function(e) {
                            e.stopPropagation();
                          }, { passive: false });
                          
                          button.addEventListener('touchmove', function(e) {
                            e.stopPropagation();
                          }, { passive: false });
                          
                          button.addEventListener('touchend', function(e) {
                            e.stopPropagation();
                          }, { passive: false });
                          
                          button.addEventListener('click', async function(e) {
                            e.preventDefault();
                            e.stopPropagation();
                            e.stopImmediatePropagation();
                            
                            const variantId = this.getAttribute('data-variant-id');
                            
                            if (!variantId || this.disabled) {
                              return;
                            }
                            
                            // Disable button during request
                            const originalText = this.textContent.trim();
                            this.disabled = true;
                            this.textContent = 'Adding...';
                            
                            try {
                              // Use the same fetch config pattern as product-form.js
                              const config = typeof fetchConfig !== 'undefined' ? fetchConfig('javascript') : {
                                method: 'POST',
                                headers: {
                                  'Accept': 'application/javascript'
                                }
                              };
                              config.headers['X-Requested-With'] = 'XMLHttpRequest';
                              delete config.headers['Content-Type'];
                              
                              const formData = new FormData();
                              formData.append('id', variantId);
                              formData.append('quantity', 1);
                              
                              // Check for cart drawer/notification to update
                              const cart = document.querySelector('cart-notification') || document.querySelector('cart-drawer');
                              if (cart && typeof cart.getSectionsToRender === 'function') {
                                const sectionsToRender = cart.getSectionsToRender();
                                if (sectionsToRender && sectionsToRender.length > 0) {
                                  // Request sections - use section property if available, otherwise use id
                                  // This matches how Shopify expects section requests
                                  const sectionIds = sectionsToRender.map((section) => {
                                    // Some cart components use 'section' property, others use 'id'
                                    return section.section || section.id;
                                  });
                                  formData.append('sections', sectionIds);
                                  formData.append('sections_url', window.location.pathname);
                                  if (typeof cart.setActiveElement === 'function') {
                                    cart.setActiveElement(document.activeElement);
                                  }
                                }
                              }
                              
                              config.body = formData;
                              
                              const cartAddUrl = (typeof routes !== 'undefined' && routes.cart_add_url) 
                                ? routes.cart_add_url 
                                : '/cart/add.js';
                              
                              const response = await fetch(cartAddUrl, config);
                              
                              // Check if response is ok first
                              if (!response.ok) {
                                throw new Error(`HTTP error! status: ${response.status}`);
                              }
                              
                              const data = await response.json();
                              
                              // Check for errors (Shopify returns status property on error)
                              if (data.status) {
                                // Error response
                                console.error('Add to cart error:', data.description || data.message);
                                this.textContent = data.description || 'Error';
                                setTimeout(() => {
                                  this.textContent = originalText;
                                  this.disabled = false;
                                }, 2000);
                                return;
                              }
                              
                              // Success - publish cart update event first (same as product-form.js)
                              // This notifies all cart components to update
                              if (typeof publish !== 'undefined' && typeof PUB_SUB_EVENTS !== 'undefined') {
                                publish(PUB_SUB_EVENTS.cartUpdate, {
                                  source: 'complete-system-slider',
                                  productVariantId: variantId,
                                  cartData: data,
                                });
                              }
                              
                              // Render cart contents if cart drawer exists
                              // Handle case where sections might not be in response (empty cart scenario)
                              if (cart && typeof cart.renderContents === 'function') {
                                // Remove is-empty class if present
                                if (cart.classList && cart.classList.contains('is-empty')) {
                                  cart.classList.remove('is-empty');
                                }
                                
                                // Check if sections are in response
                                const sectionsToRender = cart.getSectionsToRender();
                                const hasAllSections = sectionsToRender.every(section => {
                                  const sectionKey = section.section || section.id;
                                  return data.sections && data.sections[sectionKey];
                                });
                                
                                if (!hasAllSections) {
                                  // Sections missing - fetch them separately
                                  const sectionIds = sectionsToRender.map(s => s.section || s.id).join(',');
                                  fetch(`${window.location.pathname}?sections=${sectionIds}`)
                                    .then(res => res.json())
                                    .then(sectionData => {
                                      // Merge section data into response
                                      if (sectionData.sections) {
                                        data.sections = { ...data.sections, ...sectionData.sections };
                                      } else {
                                        data.sections = { ...data.sections, ...sectionData };
                                      }
                                      // Now render with complete data
                                      if (typeof CartPerformance !== 'undefined' && CartPerformance.measure) {
                                        CartPerformance.measure("add:paint-updated-sections", () => {
                                          cart.renderContents(data);
                                        });
                                      } else {
                                        cart.renderContents(data);
                                      }
                                    })
                                    .catch(() => {
                                      // If fetch fails, try rendering anyway (cart drawer will handle gracefully)
                                      if (typeof CartPerformance !== 'undefined' && CartPerformance.measure) {
                                        CartPerformance.measure("add:paint-updated-sections", () => {
                                          cart.renderContents(data);
                                        });
                                      } else {
                                        cart.renderContents(data);
                                      }
                                    });
                                } else {
                                  // Sections are present, render normally
                                  if (typeof CartPerformance !== 'undefined' && CartPerformance.measure) {
                                    CartPerformance.measure("add:paint-updated-sections", () => {
                                      cart.renderContents(data);
                                    });
                                  } else {
                                    cart.renderContents(data);
                                  }
                                }
                              }
                              
                              // Update button text temporarily
                              this.textContent = 'Added!';
                              setTimeout(() => {
                                this.textContent = originalText;
                                this.disabled = false;
                              }, 2000);
                            } catch (error) {
                              console.error('Add to cart error:', error);
                              this.textContent = 'Error';
                              setTimeout(() => {
                                this.textContent = originalText;
                                this.disabled = false;
                              }, 2000);
                            }
                          });
                        });
                      });
                    </script>

                    <style>
                      .complete-system-slider .slider-nav-btn:disabled {
                        cursor: not-allowed;
                      }

                      .complete-system-slider .slider-nav-btn:hover:not(:disabled) {
                        background-color: #f3f4f6;
                      }
                      
                      /* Force hide arrows when display is set to none */
                      .complete-system-slider .slider-nav-btn[style*="display: none"],
                      .complete-system-slider .slider-nav-btn[style*="display:none"] {
                        display: none !important;
                        visibility: hidden !important;
                        opacity: 0 !important;
                        pointer-events: none !important;
                      }
                      
                      /* Ensure slider items fit within container without cutoff */
                      .complete-system-slider .slider-container {
                        overflow: hidden;
                      }
                      
                      .complete-system-slider .slider-item {
                        /* Items will be sized by JavaScript based on available space */
                        min-width: 0;
                      }

                      /* Responsive adjustments */
                      @media (max-width: 640px) {
                        .complete-system-slider .slider-container {
                          margin-left: 0rem;
                          margin-right: 0rem;
                        }
                        .complete-system-slider .slider-nav-btn {
                          width: 40px;
                          height: 40px;
                        }
                        .complete-system-slider .slider-nav-btn svg {
                          width: 30px;
                          height: 30px;
                        }
                        .complete-system-slider .slider-prev {
                          left: -10px;
                        }
                        .complete-system-slider .slider-next {
                          right: -10px;
                        }
                      }
                      
                      @media (min-width: 641px) and (max-width: 1024px) {
                        .complete-system-slider .slider-container {
                          margin-left: 0rem;
                          margin-right: 0rem;
                        }
                      }
                      
                      @media (min-width: 1025px) {
                        .complete-system-slider .slider-container {
                          margin-left: 0;
                          margin-right: 0;
                        }
