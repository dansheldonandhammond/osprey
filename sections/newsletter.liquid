<style>
  .mce_inline_error {
    color: red;
    padding-top: 5px;
    font-size: 14px;
  }
  .mce_inline_success {
    color: green;
    padding-top: 5px;
    font-size: 14px;
  }
</style>
<div class="t-bg-[#f4f3f1] t-mx-auto t-py-[50px]">
  <div class="t-mt-8 t-lg:mt-0 t-max-w-[400px] t-mx-auto t-px-[20px] ">
    <a href="/" class="t-h-full t-mx-auto t-my-auto t-flex t-items-center t-justify-center"
      ><img
        src="https://www.osprey.com/gb/media/logo/websites/40/osprey_logo_1.svg"
        alt="Logo"
        class="c-header__logo parent-menu-item"
    ></a>

    <style>
      img.c-header__logo.parent-menu-item {
        filter: unset;
        max-width: 100px !important;
        width: 110px !important;
        height: auto !important;
        position: relative;
      }
      .parent-menu-item {
        color: #fff !important;
      }
      .c-header__logo {
        content: url('data:image/svg+xml;base64,');
        max-width: 100px !important;
        width: 110px !important;
        height: auto !important;
        position: relative;
      }
    </style>

    {% comment %} <h2 class="t-mx-auto t-text-center">Website Coming Soon</h2> {% endcomment %}
    <h6 class="footer-newsletter-title t-text-black t-text-[18px] lg:t-text-lg t-font-bold t-pt-3 t-uppercase t-mb-[15px]">
      Stay in touch!
    </h6>
    <div class="newsletter-signup-container t-bg-[#e8e6e1] t-py-[30px] t-px-[20px] t-rounded-lg t-shadow-md t-max-w-[500px] t-mx-auto">
  <h3 class="t-text-[20px] t-font-bold t-text-center t-mb-[15px] t-text-[#2c3e50]">Join Our Community</h3>
  <p class="t-text-center t-text-[#555] t-mb-[20px] t-text-[14px]">
    Subscribe for updates, offers, and exclusive content!
  </p>

  <form
    action="https://ospreyaustralia.us19.list-manage.com/subscribe/post-json?u=edc6e57443056e21d04674ef0&amp;id=2b1a9a4869&amp;f_id=0084c2e2f0"
    method="GET"
    id="newsletter-signup-form"
    name="newsletter-signup-form"
    class="t-flex t-flex-col t-space-y-[10px] sm:t-flex-row sm:t-space-y-0 sm:t-space-x-[10px] t-w-full"
    novalidate
  >
    <div class="t-flex-1 t-relative">
      <input
        type="email"
        name="EMAIL"
        id="newsletter-email"
        class="t-w-full t-px-[16px] t-py-[12px] t-border-[1px] t-border-[#ccc] t-rounded t-text-[16px] t-font-[400] t-outline-none focus:t-border-[#3498db] focus:t-ring-2 focus:t-ring-[#3498db] t-transition-all"
        placeholder="Your Email Address"
        tabindex="0"
        aria-label="Your Email Address"
        aria-required="true"
        required
        autocomplete="email"
      >
      <div id="newsletter-error" class="t-text-red-500 t-text-[12px] t-mt-[5px] t-h-[18px]" style="display: none;"></div>
    </div>

    <div class="t-flex t-items-center">
      <button
        type="submit"
        id="newsletter-submit"
        class="t-bg-[#3498db] hover:t-bg-[#2980b9] t-text-white t-font-[600] t-text-[16px] t-px-[20px] t-py-[12px] t-rounded t-cursor-pointer t-border-none t-transition-colors t-whitespace-nowrap"
      >
        Subscribe Now
      </button>
    </div>

    <!-- Hidden fields -->
    <div hidden>
      <input type="hidden" name="tags" value="6275277">
    </div>
    <div style="position: absolute; left: -5000px;" aria-hidden="true">
      <input type="text" name="b_edc6e57443056e21d04674ef0_2b1a9a4869" tabindex="-1" value="">
    </div>
  </form>

  <div id="newsletter-success" class="t-text-green-500 t-text-[14px] t-mt-[10px] t-text-center" style="display: none;">
    Thanks for subscribing! Check your inbox for a confirmation email.
  </div>
</div>

<script type="text/javascript">
  (function ($) {
    // Unique namespace to avoid conflicts
    window.newsletterFnames = ['EMAIL', 'FNAME', 'LNAME', 'ADDRESS', 'PHONE'];
    window.newsletterFtypes = ['email', 'text', 'text', 'address', 'phone'];

    $('#newsletter-signup-form').on('submit', function(e) {
      e.preventDefault();
      var $form = $(this);
      var email = $('#newsletter-email').val();

      // Simple email validation
      if (!email || !/^\S+@\S+\.\S+$/.test(email)) {
        $('#newsletter-error').text('Please enter a valid email address.').show();
        return;
      } else {
        $('#newsletter-error').hide();
      }

      // Submit the form via AJAX
      $.ajax({
        type: 'GET',
        url: $form.attr('action'),
        data: $form.serialize(),
        cache: false,
        dataType: 'jsonp',
        jsonp: 'c',
        contentType: 'application/json; charset=utf-8',
        success: function(data) {
          if (data.result === 'success') {
            $('#newsletter-signup-form').hide();
            $('#newsletter-success').show();
          } else {
            $('#newsletter-error').text(data.msg || 'An error occurred.').show();
          }
        },
        error: function() {
          $('#newsletter-error').text('An error occurred.').show();
        }
      });
    });
  })(jQuery);
  var $newsletterMcj = jQuery.noConflict(true);
</script>



