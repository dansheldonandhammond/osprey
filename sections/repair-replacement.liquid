                    // Helper to embed JotForm dynamically
                    function loadJotForm(url) {
                        formContainer.innerHTML = `
                            <iframe
                                allow="fullscreen;"
                                src="${url}"
                                style="min-width:100%;max-width:100%; height:1100px ;border:none;"
                                scrolling="yes"
                            ></iframe>
                            <script src='https://cdn.jotfor.ms/s/umd/latest/for-form-embed-handler.js'><\/script>
                            <script>window.jotformEmbedHandler("iframe[id='JotFormIFrame']", "https://form.jotform.com/")<\/script>
                        `;
                    }

                    // Default: show nothing
                    formContainer.innerHTML = '';

                    // Button events for all repair buttons
                    repairBtns.forEach(function(btn) {
                        btn.addEventListener('click', function(e) {
                            e.preventDefault();
                            title.textContent = 'Request a Repair';
                            loadJotForm(jotformRepair);
                        });
                    });

                    // Button events for all replacement buttons
                    replacementBtns.forEach(function(btn) {
                        btn.addEventListener('click', function(e) {
                            e.preventDefault();
                            title.textContent = 'Request a Replacement Part';
                            loadJotForm(jotformReplacement);
                        });
