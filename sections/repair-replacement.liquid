<script>
    // Accordion toggle logic with SVG icon swap
    document.addEventListener('DOMContentLoaded', function () {
        var faqItems = document.querySelectorAll('.faq-item');
        faqItems.forEach(function(item) {
            var question = item.querySelector('.faq-question');
            var content = item.querySelector('.faq-content');
            var iconContainer = item.querySelector('.faq-icon');
            question.addEventListener('click', function() {
                var isOpen = content.style.display === 'block';
                // Close all
                faqItems.forEach(function(other) {
                    other.querySelector('.faq-content').style.display = 'none';
                    var otherIcon = other.querySelector('.faq-icon');
                    if (otherIcon) {
                        otherIcon.innerHTML = `<svg class="open-icon" width="24" height="25" viewBox="0 0 24 25" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M12.1875 2.375V22.625" stroke="black" stroke-width="2" stroke-linejoin="round"></path>
                            <path d="M22.125 12.5L1.875 12.5" stroke="black" stroke-width="2" stroke-linejoin="round"></path>
                        </svg>`;
                    }
                });
                // Open this one if not already open
                if (!isOpen) {
                    content.style.display = 'block';
                    if (iconContainer) {
                        iconContainer.innerHTML = `<svg class="close-icon" width="24" height="25" viewBox="0 0 24 25" fill="none" xmlns="http://www.w3.org/2000/svg" style="transform: rotate(45deg);">
                            <path d="M12.1875 2.375V22.625" stroke="black" stroke-width="2" stroke-linejoin="round"></path>
                            <path d="M22.125 12.5L1.875 12.5" stroke="black" stroke-width="2" stroke-linejoin="round"></path>
                        </svg>`;
                    }
                }
            });
            // Keyboard accessibility
            question.addEventListener('keydown', function(e) {
                if (e.key === 'Enter' || e.key === ' ') {
                    question.click();
                }
            });
        });
    });
</script>

