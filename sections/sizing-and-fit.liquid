    <style>
      .container {
        max-width: 1300px;
        margin: 0 auto;
        padding: 20px 15px;
      }

      .heading {
        text-align: center;
        color: #2c3e50;
        font-size: 2.5rem;
        font-weight: 700;
        text-transform: uppercase;
        margin-bottom: 40px;
        letter-spacing: 1px;
      }

      .sizing-section {
        background: white;
        border-radius: 12px;
        overflow: hidden;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
      }

      .section-title {
        background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%);
        color: white;
        padding: 20px;
        font-size: 1.5rem;
        font-weight: 700;
        text-transform: uppercase;
        letter-spacing: 0.5px;
      }

      .table-container {
        position: relative;
        overflow-x: auto;
        -webkit-overflow-scrolling: touch;
        scrollbar-width: thin;
        scrollbar-color: #cbd5e0 #f1f5f9;
      }

      .table-container::-webkit-scrollbar {
        height: 8px;
      }

      .table-container::-webkit-scrollbar-track {
        background: #f1f5f9;
      }

      .table-container::-webkit-scrollbar-thumb {
        background: #cbd5e0;
        border-radius: 4px;
      }

      .table-container::-webkit-scrollbar-thumb:hover {
        background: #a0aec0;
      }

      .sizing-table {
        width: 100%;
        border-collapse: separate;
        border-spacing: 0;
        min-width: 1200px; /* Ensures table doesn't compress too much */
        font-size: 14px;
      }

      .sizing-table th,
      .sizing-table td {
        padding: 12px 8px;
        text-align: center;
        vertical-align: middle;
        border-bottom: 1px solid #e2e8f0;
        white-space: nowrap;
      }

      /* Header row with torso measurements */
      .torso-header {
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
        color: white;
        font-weight: 700;
        font-size: 12px;
        position: sticky;
        top: 0;
        z-index: 10;
      }

      .torso-header th:first-child {
        background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%);
        text-transform: uppercase;
        font-size: 14px;
        letter-spacing: 0.5px;
      }

      /* Category headers */
      .category-header {
        background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%);
        color: white;
        font-weight: 700;
        text-transform: uppercase;
        font-size: 16px;
        letter-spacing: 0.5px;
      }

      .category-header td {
        padding: 16px 20px;
      }

      /* Product rows */
      .product-row {
        transition: background-color 0.2s ease;
      }

      .product-row:nth-child(even) {
        background-color: #f8fafc;
      }

      .product-row:hover {
        background-color: #e2e8f0;
      }

      /* Fixed first column */
      .sizing-table th:first-child,
      .sizing-table td:first-child {
        position: sticky;
        left: 0;
        z-index: 5;
        background: white;
        border-right: 2px solid #e2e8f0;
        font-weight: 600;
        text-align: left;
        padding-left: 20px;
        min-width: 200px;
        max-width: 200px;
      }

      .category-header td:first-child {
        background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%);
        color: white;
      }

      .torso-header th:first-child {
        background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%);
      }

      .product-row:nth-child(even) td:first-child {
        background: #f8fafc;
      }

      .product-row:hover td:first-child {
        background: #e2e8f0;
      }

      /* Size indicators */
      .size-sm {
        color: #059669;
        font-weight: 600;
      }
      .size-ml {
        color: #dc2626;
        font-weight: 600;
      }
      .size-lxl {
        color: #7c3aed;
        font-weight: 600;
      }
      .size-os {
        color: #ea580c;
        font-weight: 600;
      }
      .size-unavailable {
        color: #9ca3af;
      }

      /* Mobile responsive adjustments */
      @media (max-width: 768px) {
        .container {
          padding: 15px 10px;
        }

        .heading {
          font-size: 2rem;
          margin-bottom: 30px;
        }

        .section-title {
          font-size: 1.25rem;
          padding: 15px;
        }

        .sizing-table {
          min-width: 1000px;
          font-size: 13px;
        }

        .sizing-table th,
        .sizing-table td {
          padding: 10px 6px;
        }

        .sizing-table th:first-child,
        .sizing-table td:first-child {
          min-width: 180px;
          max-width: 180px;
          padding-left: 15px;
        }

        .torso-header {
          font-size: 11px;
        }

        .category-header {
          font-size: 14px;
        }

        .category-header td {
          padding: 12px 15px;
        }
      }

      @media (max-width: 480px) {
        .heading {
          font-size: 1.75rem;
        }

        .sizing-table {
          font-size: 12px;
        }

        .sizing-table th:first-child,
        .sizing-table td:first-child {
          min-width: 160px;
          max-width: 160px;
          padding-left: 12px;
        }
      }

      /* Scroll hint for mobile */
      .scroll-hint {
        text-align: center;
        color: #64748b;
        font-size: 14px;
        margin-bottom: 10px;
        display: none;
      }

      @media (max-width: 768px) {
        .scroll-hint {
          display: block;
        }
      }
    </style>

